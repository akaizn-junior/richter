{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "default": {
            "outputSourcePath": false,
            "outputSourceFiles": false
        }
    },
    "opts": {
        "destination": "docs",
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "recursive": true
    }
}
